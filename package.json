{
  "name": "styled-bootstrap",
  "version": "0.1.4",
  "private": false,
  "description": "--",
  "main": "lib/main.js",
  "module": "lib/main.es.js",
  "author": "Jose Miguel Bejarano <yo@josemiguel.org>",
  "homepage": "https://github.com/xDae/styled-bootstrap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xDae/styled-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/xDae/styled-bootstrap/issues"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "rimraf lib && rollup -c",
    "dev": "lerna exec BABEL_ENV=production --parallel -- babel src -d es --ignore tests,__tests__,test.js,stories/,story.jsx --plugins \"transform-runtime\" --copy-files -w",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "prettier": "prettier --single-quote --write 'src/**/*.js'",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "rimraf docs && build-storybook -o docs",
    "lint:css": "stylelint './src/components/**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "styled-components": "2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-centered": "^3.2.7",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/addon-options": "^3.2.6",
    "@storybook/addons": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "aria-query": "0.7.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "^2.1.0",
    "cross-env": "^5.0.5",
    "enzyme": "2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.53.1",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "jest-styled-components": "^4.4.1",
    "lerna": "^2.1.0",
    "lint-staged": "^4.0.4",
    "polished": "^1.6.1",
    "prettier": "1.5.3",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.0",
    "react-dev-utils": "^3.1.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "^15.6.1",
    "react-transition-group": "2.2.0",
    "rimraf": "2.6.1",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "shelljs": "0.7.8",
    "stylelint": "8.0.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-processor-styled-components": "^0.4.0"
  },
  "peerDependencies": {
    "polished": "^1.1.3",
    "prop-types": "15.x",
    "react": "15.x",
    "recompose": "^0.23.5"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/components",
      "<rootDir>/utils"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(lib|es|docs|node_modules)/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
