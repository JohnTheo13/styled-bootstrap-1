{
  "name": "styled-bootstrap",
  "version": "0.1.5-0",
  "private": false,
  "description": "--",
  "homepage": "https://github.com/xDae/styled-bootstrap",
  "bugs": {
    "url": "https://github.com/xDae/styled-bootstrap/issues"
  },
  "license": "MIT",
  "author": "Jose Miguel Bejarano <yo@josemiguel.org>",
  "files": [
    "lib",
    "dist"
  ],
  "main": "dist/main.js",
  "module": "dist/main.es.js",
  "jsnext:main": "dist/main.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/xDae/styled-bootstrap.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build-bundle && npm run build-packages",
    "clean": "rimraf lib && rimraf dist",
    "build-bundle": "rollup -c",
    "build-packages": "babel src/ --out-dir lib/ --copy-files",
    "build-storybook": "rimraf docs && build-storybook -o docs",
    "prepublish": "npm run build",
    "lint:css": "stylelint './src/components/**/*.js'",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9009",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "prettier": "prettier --single-quote --write 'src/**/*.js'"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-knobs": "^3.3.13",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/addon-options": "^3.3.12",
    "@storybook/addons": "^3.3.12",
    "@storybook/react": "^3.3.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "^2.3.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.65.0",
    "jest": "^22.2.1",
    "jest-styled-components": "^4.10.0",
    "lint-staged": "^6.1.0",
    "polished": "^1.9.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.55.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-visualizer": "^0.3.1",
    "storybook-host": "^4.1.5",
    "styled-components": "^3.1.6",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-processor-styled-components": "^1.2.2"
  },
  "peerDependencies": {
    "polished": "^1.1.3",
    "react": "15.x",
    "recompose": "^0.23.5",
    "styled-components": "^3.1.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/components",
      "<rootDir>/utils"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(lib|es|docs|node_modules)/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "workspaces": [
    "components/*",
    "utils"
  ]
}
