{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-2"
  ],
  "plugins": [
    "polished",
    "external-helpers",
    "syntax-flow"
  ],
  "env": {
    "production": {
      "presets": [
        "env",
        "react",
        "stage-2"
      ],
      "plugins": ["external-helpers"]
    },
    "test": {
      "presets": ["env"]
    }
  }
}
